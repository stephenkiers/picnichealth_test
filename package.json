{
  "name": "react-node-server",
  "version": "0.0.2",
  "engines": {
    "node": "8.1.2",
    "npm": "5.0.3"
  },
  "description": "Interview React Setup.",
  "main": "./src/entry.js",
  "author": "Stephen Kiers",
  "license": "All Rights Reserved",
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.18.0",
    "compression": "^1.6.2",
    "cookies-js": "^1.2.3",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "faker": "^4.1.0",
    "file-loader": "^0.9.0",
    "form-data": "^2.1.2",
    "immutable": ">=4.0.0-rc.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.10.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.1",
    "redux": "^3.5.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.81.0",
    "url-loader": "^0.5.6",
    "webpack": "^3.0.0"
  },
  "scripts": {
    "dev-server": "NODE_ENV=development babel-node app.js --presets es2015,stage-2",
    "dev-css": "node-sass --include-path scss ./src/app/app.scss   public/cdn-generated/reactapp.css --watch ./src --source-comments true --source-map true --output-style nested --source-map-embed true",
    "dev-js": "webpack --config ./webpack.dev.config.js --watch --progress --colors",
    "tests_break": "================================================================================================",
    "test": "jest",
    "test-watch": "jest --watch",
    "heroku_scripts_break": "================================================================================================",
    "prod-css": "node-sass --include-path scss ./src/app/app.scss   public/cdn-generated/reactapp.css --output-style compressed",
    "prod-js": "webpack --config ./webpack.prod.config.js --progress --colors --display-error-details",
    "start": "NODE_ENV=production ./node_modules/.bin/babel-node app.js --presets es2015,stage-2",
    "postinstall": "npm run prod-js && npm run prod-css"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "enzyme": "^2.9.1",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3"
  }
}
